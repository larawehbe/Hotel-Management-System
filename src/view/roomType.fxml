      <Button fx:id="update" layoutX="239.0" layoutY="296.0" mnemonicParsing="false" style="-fx-background-color: #c1c9e4;" text="Update" textFill="#2a1b3d">
          <font>
              <Font name="Agency FB Bold" size="20.0" />
          </font>
      </Button>
      <Button fx:id="delete" layoutX="410.0" layoutY="296.0" mnemonicParsing="false" style="-fx-background-color: #c1c9e4;" text="Delete" textFill="#2a1b3d">
          <font>
              <Font name="Agency FB Bold" size="20.0" />
          </font>
      </Button>
   </children>
</AnchorPane>
