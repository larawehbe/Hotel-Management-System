              <Font name="Agency FB Bold" size="20.0" />
          </font>
      </Button>
      <TableView fx:id="table" layoutX="175.0" layoutY="28.0" prefHeight="200.0" prefWidth="232.0" style="-fx-background-color: #c1c9e4;">
        <columns>
          <TableColumn fx:id="roomtype" prefWidth="75.0" text="Type ID" />
          <TableColumn fx:id="category" prefWidth="75.0" text="Category" />
            <TableColumn fx:id="descr" prefWidth="75.0" text="Description" />
